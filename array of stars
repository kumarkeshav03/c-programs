// program to print a array of stars using nested loops
#include <stdio.h>

int main() {
    int i,j,row,col;
     printf("enter the number of rows:");
     scanf("%d",&row);
    
    printf("enter the numbwer of columns:");
    scanf("%d",&col);
    
    
    for (i=0;i<=row;i++){
        for(j=0;j<=col;j++){
            printf("*");
        }
            printf("\n"); // his will make to print the stars in new line so that not all the starts ar pinted in same line
        
    }
        
    

    return 0;
}
